<configuration>
  <system.webServer>

    <handlers>
      <add name="iisnode" path="bundle.js" verb="*" modules="iisnode" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="myapp">
          <match url="/*" />
          <action type="Rewrite" url="bundle.js" />
        </rule>
      </rules>
    </rewrite>

  <iisnode nodeProcessCommandLine="C:\Program Files\nodejs\node.exe" />
  </system.webServer>
</configuration>